{
  "name": "Aquerium",
  "description" : "GitHub query management tool",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "temp_logo.png"
  },
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens index.html"
    }
  }
}
